{
  "model": {
    "default": "gemini-3-pro-preview"
  },
  "enablePreview": true,
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest",
        "--browser-url",
        "http://127.0.0.1:9222"
      ]
    }
  },
  "tools": {
    "allowed": [
      "ls -F",
      "git status",
      "git diff",
      "git log",
      "pwd",
      "cat",
      "ls -la",
      "grep",
      "find",
      "head",
      "tail",
      "wc"
    ]
  }
}